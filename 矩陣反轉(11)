#include<stdio.h>

int a[100][100],b[100][100],n,m,i,j;

int main(){
  while(scanf("%d%d",&n,&m)!=EOF){
    if(n==0&&m==0){
      break;
    }
    else {
      for(i=0;i<n;i++){
        for(j=0;j<m;j++){
          scanf("%d",&a[i][j]);
          b[j][i]=a[i][j];
        }
      }
    }
    for(i=0;i<m;i++){
      for(j=0;j<n-1;j++){
        printf("%d ",b[i][j]);
      }
      printf("%d\n",b[i][n-1]);
    }
  }
  return 0;
}
