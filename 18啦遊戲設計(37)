#include<stdio.h>

int main(){
  //int 1[10],2[10],3[10],4[10],5[10],6[10];
  int n_1=0,n_2=0,n_3=0,n_4=0,n_5=0,n_6=0;
  int b[100];
  int a,flag=0,i;
  for(i=0;i<4;i++){
    scanf("%d",&a);
    if(a==1){
      n_1++;
    }
    if(a==2){
      n_2++;
    }
    if(a==3){
      n_3++;
    }
    if(a==4){
      n_4++;
    }
    if(a==5){
      n_5++;
    }
    if(a==6){
      n_6++;
    }
  }
  b[0]=n_1;
  b[1]=n_2;
  b[2]=n_3;
  b[3]=n_4;
  b[4]=n_5;
  b[5]=n_6;
  for(i=0;i<6;i++){
    if(b[i]==4){
      printf("WIN\n");
      flag=2;
      break;
    }
    else if(b[i]>1&&b[i]!=3){
      flag=1;
    }
  }
  if(flag==0){
    printf("R\n");
  }
  /*if(n_1==4||n_2==4||n_3==4||n_4==4||n_5==4||n_6==4){
    flag=2;
    printf("WIN\n");
  }
  if(n_1>=2||n_2>=2||n_3>=2||n_4>=2||n_5>=2||n_6>=2){
    flag=1;
  }
  if(flag==0){
    printf("R\n");
  }*/
  if(flag==1){
    int ans=0,cnt=0;
    for(i=0;i<6;i++){
      if(b[i]==2){
        cnt++;
      }
    }
    if(cnt==2){
      for(i=5;i>=0;i--){
        if(b[i]==2){
          printf("%d\n",2*(i+1));
          break;
        }
      }
    }
    if(cnt==1){
      for(i=0;i<6;i++){
        if(b[i]==1){
          ans+=(i+1);
        }
      }
      printf("%d\n",ans);
    }
  }
  return 0;
}
