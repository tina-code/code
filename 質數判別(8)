#include<iostream>
using namespace std;

void main(){
  int prime,i;
  cin >> prime;
  for(i=2;i<prime;i++){
    if(prime%i==0){
      cout << "NO\n";
      break;
    }
    if(i==prime-1 && prime%i!=0){
      cout << "YES\n";
      break;
    }
  }
  if(prime==1 || prime==2){
     cout << "YES\n";
  }
}
