#include<stdio.h>

int main(){
  int a,b,ans;
  while(scanf("%d%d",&a,&b)!=EOF){
    printf("%d+%d=%d\n",a,b,a+b);
    printf("%d*%d=%d\n",a,b,a*b);
    printf("%d-%d=%d\n",a,b,a-b);
    if(a>0){
      printf("%d/%d=%d...%d\n",a,b,a/b,a%b);
    }
    else {
      int q=a/b;
      if(q<=0){
        q-=1;
      }
      else {
        q+=1;
      }
      printf("%d/%d=%d...%d\n",a,b,q,a-b*q);
    }
  }
}
